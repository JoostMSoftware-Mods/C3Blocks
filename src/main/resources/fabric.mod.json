{
  "schemaVersion": 1,
  "id": "c3blocks",
  "version": "${version}",
  "name": "C3 Blocks",
  "description": "A simple compress block mod that adds all the compressed blocks of your desires",
  "authors": [
    "KirboSoftware"
  ],
  "contact": {
    "issues": "https://github.com/KirboSoftware-Mods/C3Blocks/issues",
    "sources": "https://github.com/KirboSoftware-Mods/C3Blocks"
  },

  "license": "MIT",
  "icon": "assets/c3blocks/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kirbosoftware.c3blocks.C3"
    ],
    "client": [
